{
  "rewrites": [
    {
      "source": "/src/js/(.*)",
      "destination": "/src/js/$1"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false,
  "outputDirectory": "public"
}
